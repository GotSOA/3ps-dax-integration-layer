{
   "type":"object",
   "$schema":"http://json-schema.org/draft-04/schema",
   "required":true,
   "properties":{
      "company":{
         "type":"object",
         "required":true,
         "properties":{
         	"companyBHNSetupComplete":{
         	   "type":"boolean"
         	},
            "companyCode":{
               "type":"string",
               "required":true
            },
            "companyCountry":{
               "type":"string",
               "required":true
            }
         }
      },
      "cpDivision":{
         "type":"object",
         "required":true,
         "properties":{
            "accountType":{
               "type":"string"
            }
         }
      },
      "processorInformation":{
         "type":"object",
         "required":true,
         "properties":{
            "processorContactFirstName":{
               "type":"string"
            }
         }
      },
      "CustomerStore":{
         "type":"array",
         "required":true,
         "items":{
            "type":"object",
            "properties":{
               "productLanguage":{
                  "type":"string"
               }
            }
         }
      },
      "ProductInformation":{
         "type":"array",
         "required":true,
         "items":{
            "type":"object",
            "properties":{
               "productBuyerGroup":{
                  "type":"string"
               },
               "iid":{
                  "type":"object",
                  "required":true
               }
            }
         }
      },
      "vendorInformation":{
         "type":"object",
         "required":true,
         "properties":{
            "vendorCompanyLegalName":{
               "type":"string"
            }
         }
      }
   }
}